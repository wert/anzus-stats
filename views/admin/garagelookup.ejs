<%- include("../default/head") %>
<script type="text/javascript" src="/js/glookup.js"></script>
<script type="text/javascript" src="/js/underscore-min.js">
</script>
<script src="/js/jquery.min.js"></script>
<script type="text/javascript" src="/js/jquery.dataTables.min.js"></script>

<link href="/css/jquery.dataTables.min.css" rel="stylesheet">
<link href="/css/select2.min.css" rel="stylesheet" />
<script src="/js/select2.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/clipboard.js/2.0.4/clipboard.min.js"></script>

<div class="c-body">
    <main class="c-main">
        <div class="container-fluid">
            <div id="ui-view">
                <div>
                    <div class="fade-in">
                        <div class="row1">
                            <div id="loaderDiv">
                                <div class="lds-hourglass"></div>
                            </div>
                            <div class="col-sm-6" id="mainSearch" style="margin: auto;">
                                <div class="card">
                                    <div class="card-header">Garage Lookup</div>
                                    <div class="card-body">
                                        <div class="row">
                                            <div class="col-sm-12">
                                                <div class="form-group">
                                                    <label for="steamid" style="color: #fff;">Steam ID <small
                                                            style="color:red;font-weight: normal;">(required)</small></label>
                                                    <input class="form-control" id="steamid" style="color:white"
                                                        type="text" placeholder="76561198880624547">
                                                </div>
                                            </div>
                                        </div>
                                        <button class="btn px-4" style="color:#fff;box-shadow:unset" id="searchBtn"
                                            type="button">Search</button>
                                    </div>
                                </div>
                            </div>
                            <div class="card" id="garageStuff" style="margin:auto;width:70%;margin-bottom: 10px;">
                                <div class="card-header"> User Garage
                                </div>
                                <div class="card-body">
                                    <div class="row">
                                        <div class="col-sm-12">
                                            <table id="garageTable" style="background-color:#242424"
                                                class="table table-striped table-bordered datatable dataTable no-footer">
                                                <thead>
                                                    <tr>
                                                        <th>Side</th>
                                                        <th>Classname</th>
                                                        <th>Type</th>
                                                        <th>Alive</th>
                                                        <th>Active</th>
                                                        <th>Impounded</th>
                                                        <th>Insured</th>
                                                        <th>Insert Time</th>
                                                    </tr>
                                                </thead>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </main>
</div>
<%- include("../default/footer") %>